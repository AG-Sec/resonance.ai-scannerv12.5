#@title=Resonance Scanner Config

#: Discord Bot Token
#@validate=ANY
BOT_TOKEN=PUT_YOUR_DISCORD_BOT_TOKEN

#: Discord Channel ID
#@validate=ANY
CHANNEL_ID=PUT_CHANNEL_ID_HERE

#: Lookback Hours
#@validate=INT
LOOKBACK_HOURS=95

#: Exchange (e.g., COINBASE, BINANCE, KRAKEN)
#@choices=COINBASE|BINANCE|KRAKEN
EXCHANGE=COINBASE

#: Quote Preference (e.g., USD, USDT)
#@choices=USD|USDT|BTC
QUOTE_PREFERENCE=USD

#: Timeframe (seconds)
#@validate=INT
TIMEFRAME_SEC=60

#: Take Profit % (0.10 = 10%)
#@validate=FLOAT
TP_PCT=0.10

#: Stop Loss % (0.04 = 4%)
#@validate=FLOAT
SL_PCT=0.04

#: Fee % per side
#@validate=FLOAT
FEE_PCT_PER_SIDE=0.0005

#: Lookahead Minutes
#@validate=INT
LOOKAHEAD_MINUTES=240

#: Entry Delay (seconds)
#@validate=INT
ENTRY_DELAY_SECS=0

#: Intrabar RSI Enabled
#@validate=BOOL
INTRABAR_RSI=False

#: Alerts CSV file name
#@validate=PATH
ALERTS_CSV=alerts_trades.csv

#: Results CSV file name
#@validate=PATH
RESULTS_CSV=results.csv

#: Number of sample trades to show
#@validate=INT
SHOW_SAMPLE_TRADES=3
